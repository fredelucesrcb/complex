{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "rcbfredeluces/complexclient",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "rcbfredeluces/complexserver",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "rcbfredeluces/complexworker",
            "hostname": "worker",
            "essential":false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "rcbfredeluces/complexworker",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"]
        }
    ]
}